<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PriceAndQuantityNotZero</fullName>
    <active>true</active>
    <errorConditionFormula>(UnitPrice &lt;= 0) || (Quantity &lt;= 0)</errorConditionFormula>
    <errorMessage>Please specify a valid price and quantity.</errorMessage>
</ValidationRule>
